{
  "name": "saas-template",
  "version": "1.0.0",
  "description": "A comprehensive SaaS template with frontend and backend.",
  "main": "index.js",
  "scripts": {
    "install:backend": "cd backend && npm install",
    "frontend": "vite",
    "backend": "node backend/server.cjs",
    "dev": "concurrently \"npm run frontend\" \"npm run dev:backend\"",
    "dev:backend": "dotenv -e .env.local -- node backend/server.cjs",
    "start": "concurrently \"npm run frontend\" \"npm run backend\""
  },
  "keywords": [
    "saas",
    "template",
    "react",
    "node",
    "express"
  ],
  "author": "Your Name",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.15.0",
    "@supabase/supabase-js": "^2.86.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "vite": "^5.4.21"
  }
}