    "@google/generative-ai": "^0.24.1",
    "@supabase/supabase-js": "^2.86.0",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "framer-motion": "^12.23.25",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.344.0",
    "mercadopago": "^2.11.0",
