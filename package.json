{
  "name": "saas-template",
  "version": "1.0.0",
  "description": "A comprehensive SaaS template with frontend and backend.",
  "main": "index.js",
  "scripts": {
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && npm install",
    "install:all": "npm run install:frontend && npm run install:backend",
    "frontend": "vite",
    "backend": "node backend/server.cjs",
    "dev": "concurrently \"npm run frontend\" \"npm run dev:backend\"",
    "dev:backend": "node backend/server.cjs",
    "start": "concurrently \"npm run frontend\" \"npm run backend\""
  },
  "keywords": [
    "saas",
    "template",
    "react",
    "node",
    "express"
  ],
  "author": "Your Name",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {}
}